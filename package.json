    "url": "https://github.com/rohithkutty/lucifer-on-wheels/issues"
  },
  "homepage": "https://github.com/rohithkutty/lucifer-on-wheels#readme",
  "dependencies": {
    "babel-eslint": "^8.2.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^5.0.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.5",
    "passport": "^0.4.0",
